{
	"source": "src/",
	"build": {
		"bin": {
			"destination": "bin/",
			"package": [
				"index.html",
				"manifest.json",
				"background.js",
				"favicon.ico",
				"css/",
				"font/",
				"img/",
				"lib/"
			],
			"task": [
				"remakeDest -> ",
				"copyPkg -> root.build.bin.package",
				"consolidateCode -> root.code"
			]
		},
		"web": {
			"destination": "docs/",
			"package": [
				"index.html",
				"favicon.ico",
				"css/main.css",
				"img/icon.png",
				"lib/"
			],
			"task": [
				"remakeDest -> ",
				"makeDir -> css/",
				"makeDir -> img/",
				"copyPkg -> root.build.web.package",
				"consolidateCode -> root.code",
				"replaceText -> index.html, css/material-icons.css, https://fonts.googleapis.com/icon?family=Material+Icons",
				"renameDir -> js, brn",
				"replaceText -> index.html, js/main.js, brn/main.js",
				"renameDir -> css, dzn",
				"replaceText -> index.html, css/main.css, dzn/main.css"
			]
		}
	},
	"code": {
		"directory": "js/",
		"files": [
			"utility.js",
			"preferences.js",
			"toast.js",
			"status.js",
			"xhrlog.js",
			"extract.js",
			"controls/controlstab.js",
			"controls/controlsmenu.js",
			"controls/controlsdepot.js",
			"controls/controls.js",
			"appbar.js",
			"usertoken.js",
			"sendmessage.js",
			"outboundsms.js",
			"callsubscription.js",
			"presence/callpresence.js",
			"presence/callpresencelistsubscriptions.js",
			"presence/updateownstatus.js",
			"presence/watchuserstatus.js",
			"presence/adhocpresencelist.js",
			"addressbook/contacts.js",
			"addressbook/searchcontact.js",
			"addressbook/updatecontact.js",
			"websocketconnection.js",
			"userchannel.js",
			"webrtc/webrtcsubscription.js",
			"calls/sdpgenerate.js",
			"calls/voicecall.js",
			"calls/answercall.js",
			"calls/endcall.js",
			"calls/holdcall.js",
			"2fa/smssend.js",
			"2fa/smsverify.js",
			"2fa/emailsend.js",
			"2fa/emailverify.js",
			"main.js"
		],
		"consolidate": "main.js"
	}
}